<div>
    {{!-- <h1 class="card z-depth-3 center-align">Read the latest Android News!</h1> --}}
    <div class="row">
      <div class="col s3"></div>
      <h5 class="card z-depth-3 center-align col s6">Click the button below to get articles.</h5>
    </div>

   <div class="row">
      <div class="col s12 center-align">
      <button class = "center-align"><a style ="color: black" href="/scrape">Scrape</a></button>
      
      </div>
    </div>
   
</div>

<div class="row">
  <div class="col s2"></div>
  <div class ="col s8 left-align">
    {{#each article}}
      <div class="card-panel">
        <h4>{{this.title}}</h4>
        {{!-- goes to article.handlebars --}}
        <a href = "/viewarticle/{{this._id}}" class="waves-effect waves-light btn green">Read More</a>
      </div>
    {{/each}}
  </div>
</div>